{
  "name": "@edx/frontend-enterprise",
  "version": "1.0.0-semantically-released",
  "description": "Frontend utilities for supporting enterprise features.",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "watch": {
    "build": "src/*.js"
  },
  "scripts": {
    "build": "NODE_ENV=production BABEL_ENV=production webpack --config webpack.config.js",
    "gc": "commit",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint": "eslint --ext .js --ext .jsx .",
    "is-es5": "es-check es5 ./dist/*.js",
    "precommit": "npm run lint",
    "prepublishOnly": "npm run build",
    "semantic-release": "semantic-release",
    "test": "jest --coverage",
    "travis-deploy-once": "travis-deploy-once",
    "watch": "npm-watch build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edx/frontend-enterprise.git"
  },
  "author": "edX",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/edx/frontend-enterprise/issues"
  },
  "homepage": "https://github.com/edx/frontend-enterprise#readme",
  "devDependencies": {
    "@commitlint/cli": "^7.1.2",
    "@commitlint/config-angular": "^6.0.2",
    "@commitlint/prompt": "^6.0.2",
    "@commitlint/prompt-cli": "^6.0.2",
    "@edx/frontend-platform": "^1.5.2",
    "axios-mock-adapter": "^1.15.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-rewire": "^1.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "coveralls": "^3.0.0",
    "es-check": "^5.0.0",
    "eslint": "^5.2.0",
    "eslint-config-edx": "^4.0.4",
    "husky": "^0.14.3",
    "jest": "23.6.0",
    "npm-watch": "^0.6.0",
    "react": "^16.13.1",
    "semantic-release": "^15.1.7",
    "source-map-loader": "^0.2.1",
    "travis-deploy-once": "^5.0.0",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "webpack": "^4.16.2",
    "webpack-cli": "^3.1.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!**/node_modules/**",
      "!**/tests/**"
    ]
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "@edx/frontend-platform": "^1.5.2"
  },
  "dependencies": {
    "query-string": "^6.13.2"
  }
}
