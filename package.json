{
  "name": "@edx/frontend-enterprise",
  "version": "1.0.0-semantically-released",
  "description": "Frontend utilities for supporting enterprise features.",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "watch": {
    "build": "src/*.js"
  },
  "scripts": {
    "build": "make build",
    "gc": "commit",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint": "fedx-scripts eslint --ext .js --ext .jsx .",
    "precommit": "npm run lint",
    "prepublishOnly": "npm run build",
    "semantic-release": "semantic-release",
    "test": "fedx-scripts jest --coverage --passWithNoTests",
    "travis-deploy-once": "travis-deploy-once",
    "watch": "npm-watch build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edx/frontend-enterprise.git"
  },
  "author": "edX",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/edx/frontend-enterprise/issues"
  },
  "homepage": "https://github.com/edx/frontend-enterprise#readme",
  "devDependencies": {
    "@commitlint/cli": "^7.1.2",
    "@commitlint/config-angular": "^6.0.2",
    "@commitlint/prompt": "^6.0.2",
    "@commitlint/prompt-cli": "^6.0.2",
    "@edx/frontend-build": "^5.3.2",
    "@edx/frontend-platform": "^1.5.2",
    "@edx/paragon": "^12.0.5",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.2",
    "@testing-library/react-hooks": "^3.4.2",
    "axios-mock-adapter": "^1.15.0",
    "coveralls": "^3.0.0",
    "eslint-plugin-import": "^2.22.1",
    "history": "^4.7.2",
    "husky": "^0.14.3",
    "jest": "23.6.0",
    "npm-watch": "^0.6.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^17.0.1",
    "react-instantsearch-dom": "^6.8.2",
    "react-loading-skeleton": "^2.1.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-test-renderer": "^17.0.1",
    "redux": "^4.0.0",
    "semantic-release": "^17.2.3",
    "source-map-loader": "^0.2.1",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "webpack": "^4.16.2",
    "webpack-cli": "^3.1.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!**/node_modules/**",
      "!**/tests/**"
    ]
  },
  "peerDependencies": {
    "@edx/frontend-platform": "^1.5.2",
    "@edx/paragon": "^12.0.5",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-loading-skeleton": "^2.1.1",
    "react-instantsearch-dom": "^6.8.2",
    "redux": "^4.0.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "query-string": "^5.1.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-brands-svg-icons": "^5.11.2",
    "@fortawesome/free-regular-svg-icons": "^5.12.1",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@fortawesome/react-fontawesome": "^0.1.4"
  }
}
