    "@testing-library/react": "11.2.6",
    "@testing-library/react-hooks": "3.4.2",
    "@testing-library/user-event": "^13.1.5",
    "algoliasearch": "^4.8.5",
    "axios-mock-adapter": "1.19.0",
    "classnames": "^2.2.5",
    "coveralls": "3.1.0",
    "eslint-plugin-import": "2.22.1",
    "history": "4.10.1",
    "husky": "0.14.3",
    "jest": "26.6.3",
    "lodash.debounce": "^4.0.8",
    "npm-watch": "0.6.0",
    "prop-types": "15.7.2",
